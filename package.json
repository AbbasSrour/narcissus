{
    "name": "portfolio",
    "version": "0.0.1",
    "description": "Personal Portfolio Page",
    "keywords": [
        "react",
        "typescript",
        "webpack",
        "webpack 5",
        "Threejs"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:AbbasSrour/portfolio"
    },
    "license": "MIT",
    "author": "Abbas Srour",
    "main": "webpack.config.babel.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
        "profile": "cross-env NODE_ENV=production webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
        "start": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.babel.js",
        "release": "pnpm version patch",
        "update-hooks": "pnpx simple-git-hooks",
        "prerelease": "pnpm run update-hooks",
        "postinstall": "pnpm run update-hooks"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "post-commit": "git update-index --again"
    },
    "dependencies": {
        "@types/events": "^3.0.0",
        "@types/normalize-wheel": "^1.0.0",
        "@types/three": "^0.144.0",
        "classnames": "^2.3.2",
        "events": "^3.3.0",
        "gsap": "^3.11.3",
        "normalize-wheel": "^1.0.1",
        "normalize.css": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "stats.js": "^0.17.0",
        "three": "^0.145.0",
        "tweakpane": "^3.1.0"
    },
    "devDependencies": {
        "@babel/core": "~7.19.6",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/plugin-proposal-class-properties": "~7.18.6",
        "@babel/plugin-proposal-export-default-from": "^7.18.10",
        "@babel/plugin-proposal-export-namespace-from": "~7.18.9",
        "@babel/plugin-proposal-object-rest-spread": "~7.19.4",
        "@babel/plugin-proposal-throw-expressions": "~7.18.6",
        "@babel/plugin-syntax-dynamic-import": "~7.8.3",
        "@babel/plugin-transform-runtime": "~7.19.6",
        "@babel/preset-env": "~7.19.4",
        "@babel/preset-react": "~7.18.6",
        "@babel/register": "~7.18.9",
        "@pmmmwh/react-refresh-webpack-plugin": "~0.5.8",
        "@svgr/webpack": "~6.5.0",
        "@teamsupercell/typings-for-css-modules-loader": "~2.5.1",
        "@types/react": "^18.0.22",
        "@types/react-dom": "^18.0.7",
        "@typescript-eslint/eslint-plugin": "~5.41.0",
        "@typescript-eslint/parser": "~5.41.0",
        "autoprefixer": "~10.4.12",
        "babel-loader": "~8.2.5",
        "clean-webpack-plugin": "~4.0.0",
        "copy-webpack-plugin": "~11.0.0",
        "core-js": "~3.26.0",
        "cross-env": "^7.0.3",
        "css-loader": "~6.7.1",
        "cssnano": "~5.1.13",
        "eslint": "~8.26.0",
        "eslint-config-airbnb-base": "~15.0.0",
        "eslint-config-airbnb-typescript": "~17.0.0",
        "eslint-config-prettier": "~8.5.0",
        "eslint-import-resolver-alias": "~1.1.2",
        "eslint-plugin-import": "~2.26.0",
        "eslint-plugin-jsx-a11y": "~6.6.1",
        "eslint-plugin-react": "~7.31.10",
        "eslint-plugin-react-hooks": "~4.6.0",
        "eslint-webpack-plugin": "~3.2.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "~7.2.13",
        "glsl-blend": "^1.0.3",
        "glslify-loader": "^2.0.0",
        "html-loader": "~4.2.0",
        "html-webpack-plugin": "~5.5.0",
        "import-sort-style-module-and-prefix": "~0.1.3",
        "less": "~4.1.3",
        "less-loader": "~11.1.0",
        "mini-css-extract-plugin": "~2.6.1",
        "path": "~0.12.7",
        "postcss": "~8.4.18",
        "postcss-loader": "~7.0.1",
        "prettier": "~2.7.1",
        "prettier-plugin-import-sort": "~0.0.7",
        "pretty-quick": "~3.1.3",
        "raw-loader": "^4.0.2",
        "react-refresh": "~0.14.0",
        "regenerator-runtime": "~0.13.10",
        "resolve-url-loader": "~5.0.0",
        "sass": "~1.55.0",
        "sass-loader": "~13.1.0",
        "sass-resources-loader": "~2.2.5",
        "simple-git-hooks": "^2.8.1",
        "style-loader": "~3.3.1",
        "svg-url-loader": "~8.0.0",
        "terser-webpack-plugin": "~5.3.6",
        "ts-loader": "~9.4.1",
        "typescript": "~4.8.4",
        "url-loader": "~4.1.1",
        "webpack": "~5.74.0",
        "webpack-bundle-analyzer": "~4.6.1",
        "webpack-cli": "~4.10.0",
        "webpack-dev-server": "~4.11.1",
        "webpack-merge": "~5.8.0"
    },
    "importSort": {
        ".ts, .tsx": {
            "style": "module-and-prefix",
            "parser": "typescript"
        },
        ".js, .jsx": {
            "style": "module-and-prefix",
            "parser": "babylon"
        }
    }
}
